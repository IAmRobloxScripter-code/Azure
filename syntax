(const BASE_VALUE int 5)

(fn double (value int) int
    (let result int (* value 2))
    (ret result)
)

(fn main int ()
    (let result int (double BASE_VALUE))
    (let result []int ([] 1 2 3 4 5))
    (() name args...)
    (result 2)

    (loop
        (is (< result 50)
            (= result (double result))
            (stop)
            (continue)
        )
    )

    (ret 0)
)

